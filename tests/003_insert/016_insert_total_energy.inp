system rm -f 016_insert_total_energy.db
connect 016_insert_total_energy.db

insert litref bibtex ../zz_source/bsip2.bib
insert method b3lyp/6-31+G*
 gaussian_keyword method=b3lyp/6-31+G*;gbs=
